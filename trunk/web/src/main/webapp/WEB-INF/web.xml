<web-app>
	<filter>
		<filter-name>WebResourceOptimizer</filter-name>
		<filter-class>ro.isdc.wro.http.WroFilter</filter-class>
		<init-param>
    		<param-name>disableCache</param-name>
    		<param-value>true</param-value>
  		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>WebResourceOptimizer</filter-name>
		<url-pattern>/wro/*</url-pattern>		
	</filter-mapping>
</web-app>